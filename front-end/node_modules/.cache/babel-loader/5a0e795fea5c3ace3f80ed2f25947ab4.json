AA+DA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,WAAA,IAAA,GAAA,KAAA,KAAA,CAAA,KAAA,CAAA,IAAA;AACA,WAAA,KAAA,GAAA,KAAA,IAAA,CAAA,KAAA;AACA,WAAA,KAAA,GAAA,KAAA,IAAA,CAAA,KAAA;AACA,WAAA,QAAA,GAAA,KAAA,IAAA,CAAA,QAAA;AACA,KANA;AAOA,IAAA,MAPA,oBAOA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,YAAA,CAFA;;AAAA;AAGA,gBAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AAHA;AAAA;;AAAA;AAAA;AAAA;AAKA,gBAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAdA;AAeA,IAAA,kBAfA,gCAeA;AACA,UAAA,KAAA,mBAAA,IAAA,IAAA,EAAA;AACA,aAAA,mBAAA,GAAA,KAAA;AACA,OAFA,MAGA;AACA,aAAA,mBAAA,GAAA,IAAA;AACA;AACA,KAtBA;AAuBA,IAAA,WAvBA,yBAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CAFA;;AAAA;AAOA,gBAAA,MAAA,CAAA,kBAAA;;AAPA;AAAA;;AAAA;AAAA;AAAA;AASA,gBAAA,OAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,aAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAnCA;AAoCA,IAAA,QApCA,sBAoCA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,OAAA,GAAA,EAAA;;AAEA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,QAAA,CAAA,MAAA,EAAA,EAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,QAAA,CAAA,CAAA,EAAA,IAAA;AACA,QAAA,OAAA,CAAA,IAAA,CAAA,KAAA,QAAA,CAAA,CAAA,EAAA,GAAA;AACA;;AAEA,WAAA,cAAA,GACA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,QAAA,EAAA,CACA;AACA,UAAA,MAAA,EAAA,MADA;AAEA,UAAA,eAAA,EAAA,KAAA,MAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SADA;AAFA,OADA;AAWA,KAxDA;AAyDA,IAAA,WAzDA,yBAyDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,cAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA;AAAA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AAjEA;AA/DA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <h1>Welcome {{this.fname}} {{this.lname}}</h1>\n    <button class=\"buttons\" @click=\"logout()\" id=\"logoutButton\">Logout</button>\n    <div class=\"profile\">\n      <p class=\"pinfo\">First Name: {{this.fname}}</p>\n      <p class=\"pinfo\">Last Name: {{this.lname}}</p>\n      <p class=\"pinfo\">Username: {{this.username}}</p>\n      <button class=\"buttons\" @click=\"showEditProfilePop()\">Edit Profile</button>\n    </div>\n\n\n    <transition v-if=\"showEditProfileBool\" name=\"editProfile\">\n      <div class=\"editProfile-mask\">\n        <div class=\"editProfile-wrapper\">\n          <div class=\"editProfile-container\">\n            <button id=\"exit-button\" class=\"buttons\" @click=\"showEditProfilePop()\">X</button>\n            <form class=\"pure-form pure-form-aligned\" @submit.prevent=\"editProfile()\">\n              <fieldset>\n                <legend>Edit Profile</legend>\n                <div class=\"pure-control-group\">\n                  <label for=\"fname\">First Name: </label>\n                  <input id=\"fname\" v-model=\"fname\" placeholder=\"first name\">\n                </div>\n                <div class=\"pure-control-group\">\n                  <label for=\"lname\">Last Name: </label>\n                  <input id=\"lname\" v-model=\"lname\" placeholder=\"last name\">\n                </div>\n                <div class=\"pure-control-group\">\n                  <label for=\"username\">Userame: </label>\n                  <input id=\"username\" v-model=\"username\" placeholder=\"username\">\n                </div>\n                <div class=\"pure-controls\">\n                  <button type=\"submit\" class=\"buttons\">Save</button>\n                </div>\n              </fieldset>\n            </form>\n          </div>\n        </div>\n      </div>\n    </transition>\n\n    <div class=\"doughnutDiv\">\n    <h3>Your Monthly Goals</h3>\n    <doughnut :chart-data=\"datacollection\"></doughnut>\n  </div>\n\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport Doughnut from '@/components/Doughnut'\nexport default {\n  name: \"Home\",\n  components: {\n    Doughnut,\n  },\n  data() {\n    return {\n      user: '',\n      showEditProfileBool: false,\n      fname: '',\n      lname: '',\n      username: '',\n      datacollection: null,\n      expenses: [],\n      expense: null,\n      colors: [\n        '#095246',\n        '#5AAA95',\n        '#BB9F06',\n        '#096971',\n        '#70A984',\n        '#C1A81D',\n        '#087F8C',\n        '#86A873',\n        '#CCB745',\n        '#632B30',\n        '#22&4A5',\n        '#59344F',\n        '#857E7B',\n        '#FAFAC6',\n        '#FECDAA',\n        '#253031',\n        '#C6E0FF',\n        '#BCAB79',\n        '#E072A4',\n        '#B0E298',\n        '#095246',\n        '#5AAA95',\n        '#BB9F06',\n        '#096971',\n        '#70A984',\n        '#C1A81D',\n        '#087F8C',\n        '#86A873',\n        '#CCB745',\n        '#632B30',\n        '#22&4A5',\n        '#59344F',\n        '#857E7B',\n        '#FAFAC6',\n        '#FECDAA',\n        '#253031',\n        '#C6E0FF',\n        '#BCAB79',\n        '#E072A4',\n        '#B0E298',\n      ],\n    }\n  },\n  created () {\n    this.getUser();\n    this.getExpenses();\n  },\n  methods: {\n    getUser() {\n      this.user = this.$root.$data.user;\n      this.fname = this.user.fname;\n      this.lname = this.user.lname;\n      this.username = this.user.username;\n    },\n    async logout() {\n      try {\n        await axios.delete(\"/api/users\");\n        this.$root.$data.user = null;\n      } catch (error) {\n        this.$root.$data.user = null;\n      }\n    },\n    showEditProfilePop() {\n      if(this.showEditProfileBool == true) {\n        this.showEditProfileBool = false;\n      }\n      else {\n        this.showEditProfileBool = true;\n      }\n    },\n    async editProfile() {\n      try {\n        await axios.put(\"/api/users/\" + this.$root.$data.user._id, {\n          fname: this.fname,\n          lname: this.lname,\n          username: this.username,\n        });\n        this.showEditProfilePop();\n      } catch (error) {\n        console.log(error);\n        this.error = error.response.data.message;\n      }\n    },\n    fillData () {\n      var labels = [];\n      var dataMax = [];\n\n      for(var i = 0; i < this.expenses.length; ++i) {\n        labels.push(this.expenses[i].name);\n        dataMax.push(this.expenses[i].max);\n      }\n\n      this.datacollection =\n      {\n        labels: labels,\n        datasets: [\n          {\n            labels: labels,\n            backgroundColor: this.colors,\n            data: dataMax\n          },\n        ]\n      }\n    },\n    async getExpenses() {\n      try {\n        let response = await axios.get(\"/api/expense\");\n        this.expenses = response.data;\n        this.fillData();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  }\n}\n</script>\n\n<style scoped>\n.home h1 {\n  text-align: center;\n}\n.profile {\n  text-align: center;\n  margin-bottom: 10%;\n}\n#editP {\n  text-align: center;\n}\n#logoutButton {\n  position: absolute;\n  top: 70px;\n  right: 2%;\n}\n/* deleteExpense */\n.editProfile-mask {\n  position: fixed;\n  z-index: 9998;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, .5);\n  display: table;\n  transition: opacity .3s ease;\n}\n.editProfile-wrapper {\n  display: table-cell;\n  vertical-align: middle;\n}\n.editProfile-container {\n  width: 50%;\n  margin: 0px auto;\n  padding: 10px 20px;\n  background-color: #fff;\n  border-radius: 5px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, .33);\n  transition: all .3s ease;\n  font-family: Helvetica, Arial, sans-serif;\n  text-align: center;\n}\n/*\n  * The following styles are auto-applied to elements with\n  * transition=\"addExpense\" when their visibility is toggled\n  * by Vue.js.\n  *\n  * You can easily play with the addExpense transition by editing\n  * these styles.\n  */\n.editProfile-enter {\n  opacity: 0;\n}\n.editProfile-leave-active {\n  opacity: 0;\n}\n.editProfile-enter .addCost-container,\n.editProfile-leave-active .addCost-container {\n  -webkit-transform: scale(1.1);\n  transform: scale(1.1);\n}\n.editProfile-container h1 {\n  margin-top: 0;\n}\n#exit-button {\n  position: relative;\n  left: 50%;\n  top: -5px;\n  padding: 2px 5px;\n}\n\n.doughnutDiv {\n  text-align: center;\n}\n</style>\n"],"sourceRoot":"src/views"}]}